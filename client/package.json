    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
